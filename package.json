  "author": "Evgeny Kirichenko",
  "license": "MIT",
  "private": true,

  "browserslist": [
    "last 4 versions",
    "not ie <=11"
  ],
  "scripts": {
    "prod": "rm -rf ./public && GATSBY_AWS_ENV=production GATSBY_PUBLIC_URL=https://www.prod.ecom-gatsby.com GATSBY_GA_KEY=UA-000000000-0 GATSBY_GO_KEY=GTM-XXXXXX GATSBY_GTM_ID=GTM-XXXXXXX GATSBY_YANDEX_METRIKA_KEY=00000000 GATSBY_RECAPTCHA_KEY=X_Xxx gatsby build",
    "staging": "rm -rf ./public && NODE_ENV=staging GATSBY_AWS_ENV=staging GATSBY_PUBLIC_URL=https://staging.ecom-gatsby.com GATSBY_GA_KEY=UA-000000000-0 GATSBY_GO_KEY=GTM-XXXXXX GATSBY_GTM_ID=GTM-XXXXXXX GATSBY_YANDEX_METRIKA_KEY=00000000 GATSBY_RECAPTCHA_KEY=XXXX gatsby build",
    "build": "gatsby build && ps auxw",
    "develop": "GATSBY_AWS_ENV=development GATSBY_PUBLIC_URL=https://localhost:3000 gatsby develop --https -p 3000 -H localhost",
    "start": "yarn develop",
    "aws:env": "AWS_ENV=development node ./env/deploy-env.js",
    "lint": "yarn yarn tc && yarn eslint ",
    "test": "NODE_ENV=development GATSBY_AWS_ENV=development jest",
    "clean": "rm -rf ./.cache ./node_modules ./package-lock.json ./yarn.lock && yarn",
    "tc": "npx tsc -p ./tsconfig.json --noEmit --traceResolution",
    "tcw": "yarn tc -- --watch",
    "eslint": "npx eslint \"./src/**/*.{ts,tsx,gql}\" \"./*.{js,tsx,ts,json}\" --fix --no-ignore",
    "stylelint": "npx stylelint ./src/styles/",
    "css-types": "npx tcm ./src/styles"
  },
  "lint-staged": {
    "*.tsx?$": [
      "yarn test",
      "npx tsc --noEmit",
      "npx eslint --fix",
      "git add"
    ],
    "*.module.css": [
      "npx stylelint",
      "git add"
    ]
  },
